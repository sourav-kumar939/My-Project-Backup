<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="3">
  <test name="Test1">
    <classes>
      <class name="com.practice.ParallelTesting">
      <methods>
      <include name="loginDetails"/>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
  <test name="Test2">
    <classes>
      <class name="com.practice.ParallelTesting">
      <methods>
      <include name="loginDetails"/>
       <include name="verifyLogin"/>
      </methods>
      </class>
    </classes>
  </test>
  
  <test name="Test3">
    <classes>
      <class name="com.practice.ParallelTesting">
      <methods>
      <include name="loginDetails"/>
       <include name="verifyLogin"/>
       <include name="clickInfo"/>
      </methods>
      </class>
    </classes>
  </test>
  
  
</suite> <!-- Suite -->
